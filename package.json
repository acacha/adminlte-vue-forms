{
  "name": "acacha-adminlte-vue-forms",
  "version": "0.1.0",
  "description": "Acacha Forms vue components for AdminLTE template",
  "main": "dist/acacha-adminlte-vue-forms.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .js,.vue src/",
    "lint-fix": "npm run lint -- --fix",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "build-common": "cross-env NODE_ENV=production webpack --target='common' --progress --colors",
    "build-node": "cross-env NODE_ENV=production webpack --target='node' --progress --colors",
    "dev": "cross-env NODE_ENV=development webpack --progress --colors --watch",
    "dev-no-watch": "cross-env NODE_ENV=development webpack --progress --colors",
    "prepublish": "npm run dev-no-watch && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acacha/adminlte-vue-forms.git"
  },
  "keywords": [
    "Form",
    "Javascript",
    "Vue",
    "Components",
    "Adminlte"
  ],
  "author": "Sergi Tur Badenas <sergiturbadenas@gmail.com> (http://acacha.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acacha/adminlte-vue-forms/issues"
  },
  "homepage": "https://github.com/acacha/adminlte-vue-forms#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^2.1.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.3.1",
    "rimraf": "^2.6.2",
    "vue-hot-reload-api": "^2.2.4",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.5.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.10",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "change-case": "^3.0.1",
    "inputmask": "^3.3.11",
    "moment": "^2.19.4",
    "vue": "^2.5.10",
    "vue-multiselect": "^2.0.8"
  }
}
